#include "oclib.h"

int main (int argc, array<string> argv) {
   putstr (argv[0]);
   putstr (": ");
   putstr (__FILE__);
   putchr ('[');
   putint (__LINE__);
   putstr ("] compiled ");
   putstr (__DATE__);
   putstr (" ");
   putstr (__TIME__);
   putstr ("\n");
   return 0;
}

